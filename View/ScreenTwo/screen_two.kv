<Two>:
    
    MDLabel:
        id: label_one
        text: "SCREEN TWO"
        font_style: 'H4'
        halign: 'center'
        pos_hint: {'center_y': 0.6}

    MDLabel:
        id: label_two
        text: "This is screen two"
        halign: 'center'

    MDTextField:
        id: data
        hint_text:"Data will display here"
        pos_hint: {'center_x': 0.5, 'center_y': 0.4}
        size_hint_x: .8
        multiline:True
        

    MDRoundFlatButton:
        id: change_screen
        text: "Go to screen 1"
        md_bg_color: [1, 1, 1, 1]
        pos_hint: {'center_x': 0.5, 'center_y': 0.3}
        on_release: 
            app.sm.transition.direction = "right"
            app.sm.current = 'upload'
